{
    "compilerOptions": {
      "allowJs": false,
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "jsx": "react-native",
      "module": "es2015",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "sourceMap": true,
      "target": "es5",
      "lib": ["esnext"],
      "skipLibCheck": true
    },
    "exclude": ["node_modules"],
    "include": ["App.js", "src"]
  }
